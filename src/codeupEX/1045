import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);

      int a= input.nextInt();
      int b= input.nextInt();

      System.out.printf("%d\n",a+b);
        System.out.printf("%d\n",a-b);
        System.out.printf("%d\n",a*b);
        System.out.printf("%d\n",a/b);
        System.out.printf("%d\n",a%b);
        System.out.printf("%.2f",(float)a/b);


    }
}
