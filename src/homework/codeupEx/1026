import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);

        String[] arr=input.next().split(":");

        if (arr[1].equals("00")){
System.out.println("0");
        }
        else {
            System.out.println(arr[1]);
        }

        }
        }


