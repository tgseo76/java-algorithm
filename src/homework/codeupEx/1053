import java.util.Arrays;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);

        int a = input.nextInt();

        if(a==1){
            System.out.println(0);
        } else if (a==0) {
            System.out.println(1);
        }
          


    }
}
